@{
    ViewBag.Title = "Tell";
}
<script type="text/javascript">
    $(document).ready(function () {
        $('body').css('background-image', 'url("../Content/Images/Textures/darkScratch.jpg")');
        $('#bodyContainer').removeClass("container").addClass("container-fluid");
    });
</script>

<!-- MODALS -->
<!-- ADMIN MODAL -->
<div class="modal fade" id="adminModal" data-backdrop="static" tabindex="-2" aria-labelledby="adminModalLabel" aria-hidden="false">
    <div class="modal-dialog modal-xl">
        <div class="modal-header bg-black">

        </div>
        <div class="modal-content">
            <div class="modal-body">
                <div class="container-fluid bg-black">
                    <!-- LOCK GAME -->
                    <div class="row">
                        <div class="col-3"></div>
                        <div class="col-6">
                            <button id="lockBtn" class="btn btn-lg btn-secondary border border-light mx-auto btn-block mt-3 mx-auto">LOCK GAME</button>
                        </div>
                        <div class="col-3"></div>
                    </div>
                    <hr />
                    <!-- KILL CHARACTER -->
                    <div class="row">
                        <div class="col-md-2"></div>
                        <div class="col-8 col-md-4">
                            <select id="killList" class="custom-select custom-select-lg mt-2 text-center w-100"></select>
                        </div>
                        <div class="col-4">
                            <button id="killBtn" class="btn btn-lg btn-red border border-light mx-auto btn-block my-2 mx-auto">KILL</button>
                        </div>
                        <div class="col-md-2"></div>
                    </div>
                    <hr />
                    <!-- BOOT USER -->
                    <div class="row">
                        <div class="col-md-2"></div>
                        <div class="col-8 col-md-4">
                            <select id="bootList" class="custom-select custom-select-lg mt-2 text-center w-100"></select>
                        </div>
                        <div class="col-4">
                            <button id="bootUserBtn" class="btn btn-lg btn-red border border-light mx-auto btn-block my-2 mx-auto">BOOT</button>
                        </div>
                        <div class="col-md-2"></div>
                    </div>
                    <hr />
                    <!-- CHANGE SEASON -->
                    <div class="row">
                        <div class="col-md-3"></div>
                        <div class="col-6 col-md-3">
                            <img src="~/Content/Images/Graffiti/Season-black-500x150.png" class="img-fluid mx-auto d-block" />
                        </div>
                        <div class="col-6 col-md-3">
                            <select name="Season" id="season" class="custom-select custom-select-lg mt-2 text-center w-100" value="">
                                <option value="Spring">Spring</option>
                                <option value="Summer">Summer</option>
                                <option value="Fall">Fall</option>
                                <option value="Winter">Winter</option>
                            </select>
                        </div>
                        <div class="col-md-3"></div>
                    </div>
                    <!-- CHANGE YEAR -->
                    <div class="row">
                        <div class="col-md-3"></div>
                        <div class="col-6 col-md-3">
                            <img src="~/Content/Images/Graffiti/Year-black-500x150.png" class="img-fluid mx-auto d-block" />
                        </div>
                        <div class="col-6 col-md-3">
                            <select name="Year" id="year" class="custom-select custom-select-lg mt-2 text-center w-100" value="">
                                <option disabled>[Pre-Apocalypse]</option>
                                <option value="2012">2012</option>
                                <option disabled>[The Apocalypse]</option>
                                <option value="2013">2013</option>
                                <option value="2014">2014</option>
                                <option value="2015">2015</option>
                                <option value="2016">2016</option>
                                <option value="2017">2017</option>
                                <option disabled>[The Aftermath]</option>
                                <option value="2018">2018</option>
                                <option value="2019">2019</option>
                                <option value="2020">2020</option>
                                <option value="2021">2021</option>
                                <option disabled>[Present Day]</option>
                                <option value="2022">2022</option>
                                <option value="2023">2023</option>
                                <option value="2024">2024</option>
                                <option value="2025">2025</option>
                                <option value="2026">2026</option>
                                <option value="2027">2027</option>
                                <option value="2028">2028</option>
                                <option value="2029">2029</option>
                                <option value="2030">2030</option>
                                <option value="2031">2031</option>
                                <option value="2032">2032</option>
                            </select>
                        </div>
                        <div class="col-md-3"></div>
                    </div>
                    <!-- CONFIRM DATE CHANGE -->
                    <div class="row">
                        <div class="col-4"></div>
                        <div class="col-4">
                            <button id="dateBtn" class="btn btn-lg btn-success border border-light mx-auto btn-block my-2 mx-auto">UPDATE</button>
                        </div>
                        <div class="col-4"></div>
                    </div>
                    <hr />
                    <!-- CHANGE DESCRIPTION -->
                    <div class="row">
                        <div class="col-3 d-md-none"></div>
                        <div class="col-6 col-md-4">
                            <img src="~/Content/Images/Graffiti/description-white-500x150.png" class="img-fluid mx-auto d-block">
                        </div>
                        <div class="col-3 d-md-none"></div>
                        <div class="col-12 col-md-8">
                            <textarea id="description" name="Description" rows="4" cols="50" class="rounded w-100"></textarea>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-4"></div>
                        <div class="col-4">
                            <button id="descriptionBtn" class="btn btn-lg btn-success border border-light mx-auto btn-block mt-2 mb-3 mx-auto">UPDATE</button>
                        </div>
                        <div class="col-4"></div>
                    </div>
                    <!-- MESSAGE SLOT-->
                    <div class="row">
                        <div class="col-4"></div>
                        <div id="adminAlert" class="col-4 d-none">
                            <h4 id="adminMsg" class="text-center text-success font-weight-bold"></h4>
                        </div>
                        <div class="col-4"></div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button id="adminCancelBtn" class="btn btn-lg btn-danger border border-dark mx-auto my-2 mx-auto" data-dismiss="modal">CLOSE</button>
            </div>
        </div>
    </div>
</div>
<!-- CHARACTER SHEET MODAL -->
<div class="modal fade" id="charSheetModal" tabindex="-3" aria-labelledby="charSheetModalLabel" aria-hidden="false">
    <div class="modal-dialog modal-xl">
        <div class="modal-header bg-black">
            <h4 id="charSheetHeader" class="font-weight-bold text-white text-center mx-auto"></h4>
        </div>
        <div class="modal-content">
            <div class="modal-body">

                <!-- CHARACTER SHEET -->
                <div class="row bg-black">
                    <div class="col-12">
                        <h4 id="charSheetLbl" class="text-white font-weight-bold my-2 text-center">CHARACTER SHEET</h4>
                    </div>
                </div>

                <!-- BACKGROUND || STRATEGY -->
                <div class="row bg-white">
                    <div class="col-3 px-0">
                        <h6 id="backgroundLbl" class="font-weight-bold my-3 text-center">BACKGROUND:</h6>
                    </div>
                    <div class="col-3 px-0">
                        <div class="input-group my-2 text-center">
                            <input class="form-control text-center px-0 py-0" type="text" id="background" name="Background" value="" readonly />
                        </div>
                    </div>
                    <div class="col-3 px-0">
                        <h6 id="strategyLbl" class="font-weight-bold my-3 text-center" data-shortTxt="STRATEGY:" data-longTxt="SURVIVAL STRATEGY:">STRATEGY:</h6>
                    </div>
                    <div class="col-3 px-0">
                        <div class="input-group my-2 text-center">
                            <input class="form-control text-center px-0 py-0" type="text" id="strategy" name="Strategy" value="" readonly />
                        </div>
                    </div>
                </div>
                <!-- HISTORY || HABITAT -->
                <div class="row bg-white">
                    <div class="col-3 px-0">
                        <h6 id="historyLbl" class="font-weight-bold my-3 text-center">HISTORY:</h6>
                    </div>
                    <div class="col-3 px-0">
                        <div class="input-group my-2 text-center">
                            <input class="form-control text-center px-0 py-0" type="text" id="history" name="History" value="" readonly />
                        </div>
                    </div>
                    <div class="col-3 px-0">
                        <h6 id="habitatLbl" class="font-weight-bold my-3 text-center" data-shortTxt="HABITAT:" data-longTxt="ORIGINAL HABITAT:">HABITAT:</h6>
                    </div>
                    <div class="col-3 px-0">
                        <div class="input-group my-2 text-center">
                            <input class="form-control text-center px-0 py-0" type="text" id="habitat" name="Habitat" value="" readonly />
                        </div>
                    </div>
                </div>
                <!-- DEMOGRAPHICS || ATTRIBUTES -->
                <div class="row bg-black border-top border-dark">
                    <div class="col-6">
                        <h4 id="demographicsLbl" class="text-white font-weight-bold my-2 text-center">DEMOGRAPHICS</h4>
                    </div>
                    <div class="col-6">
                        <h4 id="attributeLbl" class="text-white font-weight-bold my-2 text-center">ATTRIBUTES</h4>
                    </div>
                </div>
                <!-- NAME || WILL / END -->
                <div class="row bg-white">
                    <div class="col-3 px-0">
                        <h6 id="nameLbl" class="font-weight-bold my-3 text-center">NAME:</h6>
                    </div>
                    <div class="col-3 border-right border-dark px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="text" id="characterName" name="Name" value="" readonly />
                        </div>
                    </div>

                    <div class="col-2 px-0 bg-danger border-bottom border-dark">
                        <h6 id="willpowerLbl" class="font-weight-bold my-3 text-center">WILL</h6>
                    </div>
                    <div class="col-1 px-0 bg-danger border-bottom border-dark">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="number" id="willpower" name="Willpower" value="" readonly />
                        </div>
                    </div>
                    <div class="col-2 px-0 bg-danger border-bottom border-dark">
                        <h6 id="enduranceLbl" class="font-weight-bold my-3 text-center">END</h6>
                    </div>
                    <div class="col-1 px-0 bg-danger border-bottom border-dark">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="number" id="endurance" name="Endurance" value="" readonly />
                        </div>
                    </div>
                </div>
                <!-- DOB || MEM / STR -->
                <div class="row bg-white">
                    <div class="col-3 px-0">
                        <h6 id="birthdateLbl" class="font-weight-bold my-3 text-center" data-shortTxt="D.O.B:" data-longTxt="BIRTHDATE">BIRTHDATE:</h6>
                    </div>
                    <div class="col-3 border-right border-dark px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="text" id="birthdate" name="Birthdate" value="" readonly />
                        </div>
                    </div>

                    <div class="col-2 px-0">
                        <h6 id="memoryLbl" class="font-weight-bold my-3 text-center">MEM</h6>
                    </div>
                    <div class="col-1 px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="number" id="memory" name="Memory" value="" readonly />
                        </div>
                    </div>
                    <div class="col-2 px-0">
                        <h6 id="strengthLbl" class="font-weight-bold my-3 text-center">STR</h6>
                    </div>
                    <div class="col-1 px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="number" id="strength" name="Strength" value="" readonly />
                        </div>
                    </div>
                </div>
                <!-- SEX || LOG / AGL -->
                <div class="row bg-white">
                    <div class="col-3 px-0">
                        <h6 id="sexLbl" class="font-weight-bold my-3 text-center">SEX:</h6>
                    </div>
                    <div class="col-3 border-right border-dark px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="text" id="sex" name="Sex" value="" readonly />
                        </div>
                    </div>

                    <div class="col-2 px-0">
                        <h6 id="logicLbl" class="font-weight-bold my-3 text-center">LOG</h6>
                    </div>
                    <div class="col-1 px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="number" id="logic" name="Logic" value="" readonly />
                        </div>
                    </div>
                    <div class="col-2 px-0">
                        <h6 id="agilityLbl" class="font-weight-bold my-3 text-center">AGL</h6>
                    </div>
                    <div class="col-1 px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="number" id="agility" name="Agility" value="" readonly />
                        </div>
                    </div>
                </div>
                <!-- ETHNIC || PER / SPD -->
                <div class="row bg-white">
                    <div class="col-3 px-0">
                        <h6 id="ethnicityLbl" class="font-weight-bold my-3 text-center" data-shortTxt="ETHNIC:" data-longTxt="ETHNICITY:">ETHNIC:</h6>
                    </div>
                    <div class="col-3 border-right border-dark px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="text" id="ethnicity" name="Ethnicity" value="" readonly />
                        </div>
                    </div>

                    <div class="col-2 px-0">
                        <h6 id="perceptionLbl" class="font-weight-bold my-3 text-center">PER</h6>
                    </div>
                    <div class="col-1 px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="number" id="perception" name="Perception" value="" readonly />
                        </div>
                    </div>
                    <div class="col-2 px-0">
                        <h6 id="speedLbl" class="font-weight-bold my-3 text-center">SPD</h6>
                    </div>
                    <div class="col-1 px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="number" id="speed" name="Speed" value="" readonly />
                        </div>
                    </div>
                </div>
                <!-- HAIR || CHA / BTY -->
                <div class="row bg-white">
                    <div class="col-3 px-0">
                        <h6 id="hairColorLbl" class="font-weight-bold my-3 text-center" data-shortTxt="HAIR:" data-longTxt="HAIR COLOR:">HAIR:</h6>
                    </div>
                    <div class="col-3 border-right border-dark px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="text" id="hairColor" name="HairColor" value="" readonly />
                        </div>
                    </div>

                    <div class="col-2 px-0">
                        <h6 id="charismaLbl" class="font-weight-bold my-3 text-center">CHA</h6>
                    </div>
                    <div class="col-1 px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="number" id="charisma" name="Charisma" value="" readonly />
                        </div>
                    </div>
                    <div class="col-2 px-0">
                        <h6 id="beautyLbl" class="font-weight-bold my-3 text-center" data-shortTxt="BTY" data-longTxt="BEAUTY">BTY</h6>
                    </div>
                    <div class="col-1 px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="number" id="beauty" name="Beauty" value="" readonly />
                        </div>
                    </div>
                </div>
                <!-- EYE -->
                <div class="row bg-white">
                    <div class="col-3 px-0">
                        <h6 id="eyeColorLbl" class="font-weight-bold my-3 text-center" data-shortTxt="EYES:" data-longTxt="EYE COLOR:">EYES:</h6>
                    </div>
                    <div class="col-3 border-right border-dark px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="text" id="eyeColor" name="EyeColor" value="" readonly />
                        </div>
                    </div>

                    <div class="col-6 bg-secondary px-0">
                        <h4 id="derivedAttrLbl" class="font-weight-bold my-2 text-center" data-shortTxt="DERIVED" data-longTxt="DERIVED TRAITS">DERIVED</h4>
                    </div>
                </div>
                <!-- HAIRSTYLE || SEQ / ACT -->
                <div class="row bg-white">
                    <div class="col-3 px-0">
                        <h6 id="hairstyleLbl" class="font-weight-bold my-3 text-center">HAIRSTYLE:</h6>
                    </div>
                    <div class="col-3 border-right border-dark px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="text" id="hairStyle" name="Hairstyle" value="" readonly />
                        </div>
                    </div>

                    <div class="col-2 bg-secondary px-0">
                        <h6 id="sequenceLbl" class="font-weight-bold my-3 text-center text-white" data-shortTxt="SEQ" data-longTxt="SEQUENCE">SEQ</h6>
                    </div>
                    <div class="col-1 bg-secondary px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="number" id="sequence" name="Sequence" value="" readonly />
                        </div>
                    </div>
                    <div class="col-2 bg-secondary px-0">
                        <h6 id="actionsLbl" class="font-weight-bold my-3 text-center text-white" data-shortTxt="ACTS" data-longTxt="ACTIONS">ACTS</h6>
                    </div>
                    <div class="col-1 bg-secondary px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="number" id="actions" name="Actions" value="" readonly />
                        </div>
                    </div>
                </div>
                <!-- FACIAL || OFF / DOD -->
                <div class="row bg-white">
                    <div class="col-3 px-0 facialHairSlot">
                        <h6 id="facialHairLbl" class="font-weight-bold my-3 text-center">FACIAL HAIR:</h6>
                    </div>
                    <div class="col-3 border-right border-dark px-0 facialHairSlot">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0" type="text" id="facialHair" name="FacialHair" value="" readonly />
                        </div>
                    </div>

                    <div class="col-2 bg-secondary px-0">
                        <h6 id="offHandLbl" class="font-weight-bold my-3 text-center">OFF</h6>
                    </div>
                    <div class="col-1 bg-secondary px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0 standardSkill" type="number" name="Skill-OffHand" value="" readonly />
                        </div>
                    </div>
                    <div class="col-2 bg-secondary px-0">
                        <h6 id="dodgeLbl" class="font-weight-bold my-3 text-center">DODGE</h6>
                    </div>
                    <div class="col-1 bg-secondary px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0 standardSkill" type="number" name="Skill-Dodge" value="" readonly />
                        </div>
                    </div>
                </div>

                <!-- CHARACTER SKILLSET -->
                <div class="row bg-black">
                    <div class="col-12">
                        <h4 id="outlineLbl" class="font-weight-bold my-2 text-center text-white">CHARACTER SKILLSET</h4>
                    </div>
                </div>
                <!-- COMBAT || SOCIAL -->
                <div class="row bg-white">
                    <div class="col-6 border-right border-dark">
                        <h4 id="combatSkillsLbl" class="font-weight-bold my-2 text-center" data-longTxt="COMBAT SKILLS" data-shortTxt="COMBAT">COMBAT</h4>
                    </div>
                    <div class="col-6">
                        <h4 id="socialSkillsLbl" class="font-weight-bold my-2 text-center" data-longTxt="SOCIAL SKILLS" data-shortTxt="SOCIAL">SOCIAL</h4>
                    </div>
                </div>
                <!-- UNARM / THROW || PERSUADE / DECIEVE -->
                <div class="row bg-white">
                    <div id="combat-1" class="col-2 px-0">
                        <h6 id="unarmedLbl" class="font-weight-bold my-3 text-center">UNARM</h6>
                    </div>
                    <div id="combatVal-1" class="col-1 px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0 standardSkill" type="number" name="Skill-Unarmed" value="" readonly />
                        </div>
                    </div>
                    <div id="combat-2" class="col-2 px-0">
                        <h6 id="thrownLbl" class="font-weight-bold my-3 text-center">THROWN</h6>
                    </div>
                    <div id="combatVal-2" class="col-1 px-0 border-right border-dark">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0 standardSkill" type="number" name="Skill-Thrown" value="" readonly />
                        </div>
                    </div>

                    <div id="social-1" class="col-2 px-0">
                        <h6 id="persuadeLbl" class="font-weight-bold my-3 text-center">PERSUADE</h6>
                    </div>
                    <div id="socialVal-1" class="col-1 px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0 standardSkill" type="number" name="Skill-Persuasion" value="" readonly />
                        </div>
                    </div>
                    <div id="social-2" class="col-2 px-0">
                        <h6 id="deceiveLbl" class="font-weight-bold my-3 text-center">DECEIVE</h6>
                    </div>
                    <div id="socialVal-2" class="col-1 px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0 standardSkill" type="number" name="Skill-Deception" value="" readonly />
                        </div>
                    </div>
                </div>
                <!-- GRAPPLE / ARCHERY || GAMES / ANIMALS -->
                <div class="row bg-white">
                    <div id="combat-3" class="col-2 px-0">
                        <h6 id="grappleLbl" class="font-weight-bold my-3 text-center">GRAPPLE</h6>
                    </div>
                    <div id="combatVal-3" class="col-1 px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0 standardSkill" type="number" name="Skill-Grapple" value="" readonly />
                        </div>
                    </div>
                    <div id="combat-4" class="col-2 px-0">
                        <h6 id="archeryLbl" class="font-weight-bold my-3 text-center">ARCHERY</h6>
                    </div>
                    <div id="combatVal-4" class="col-1 px-0 border-right border-dark">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0 standardSkill" type="number" name="Skill-Archery" value="" readonly />
                        </div>
                    </div>

                    <div id="social-3" class="col-2 px-0">
                        <h6 id="gamesLbl" class="font-weight-bold my-3 text-center">GAMES</h6>
                    </div>
                    <div id="socialVal-3" class="col-1 px-0">
                        <div class="input-group my-2">
                            <input class="form-control text-center px-0 py-0 standardSkill" type="number" name="Skill-Games" value="" readonly />
                        </div>
                    </div>
                    <div id="social-4" class="col-2 px-0">
                        <h6 id="animalsLbl" class="font-weight-bold my-3 text-center">ANIMALS</h6>
                    </div>
